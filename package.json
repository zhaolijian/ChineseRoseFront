{
  "name": "chinese-rose-uniapp",
  "version": "1.0.0",
  "description": "阅记 - 纸质书笔记整理助手（uni-app多端版）",
  "main": "main.js",
  "keywords": [
    "uniapp",
    "vue3",
    "typescript",
    "miniprogram",
    "h5",
    "app",
    "reading",
    "notes",
    "ocr",
    "mindmap"
  ],
  "author": "Chinese Rose Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/chinese-rose-uniapp.git"
  },
  "scripts": {
    "dev:mp-weixin": "uni build --platform mp-weixin --mode development --watch",
    "dev:h5": "uni build --platform h5 --mode development --watch",
    "serve:h5": "UNI_PLATFORM=h5 vite --port 3000 --host 0.0.0.0",
    "dev:app": "uni build --platform app --mode development --watch",
    "build:mp-weixin": "uni build --platform mp-weixin --mode production",
    "build:h5": "uni build --platform h5 --mode production",
    "build:app": "uni build --platform app --mode production",
    "lint": "eslint . --ext .js,.ts,.vue --fix",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "postinstall": "node scripts/patches/fix-vue-demi-hasInjectionContext.js && node scripts/patches/patch-vue-tsc.js",
    "postbuild:mp-weixin": "node scripts/postbuild/create-mp-assets-stub.js"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-3090920231225001",
    "@dcloudio/uni-components": "3.0.0-3090920231225001",
    "@dcloudio/uni-mp-vue": "3.0.0-3090920231225001",
    "@vue/shared": "3.2.45",
    "crypto-js": "^4.2.0",
    "pinia": "2.1.7",
    "uview-plus": "^3.2.17",
    "vue": "3.2.45",
    "vue-demi": "^0.14.10"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.14",
    "@dcloudio/uni-automator": "3.0.0-3090920231225001",
    "@dcloudio/uni-cli-shared": "3.0.0-3090920231225001",
    "@dcloudio/uni-mp-weixin": "3.0.0-3090920231225001",
    "@dcloudio/uni-stacktracey": "3.0.0-3090920231225001",
    "@dcloudio/vite-plugin-uni": "3.0.0-3090920231225001",
    "@pinia/testing": "0.1.3",
    "@playwright/test": "^1.55.0",
    "@types/crypto-js": "^4.2.2",
    "@types/wechat-miniprogram": "^3.4.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vue/compiler-sfc": "3.2.45",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "express": "^5.1.0",
    "happy-dom": "^10.0.3",
    "miniprogram-automator": "^0.12.1",
    "miniprogram-ci": "^2.1.26",
    "msw": "^1.3.2",
    "sass": "^1.69.5",
    "sharp": "^0.34.3",
    "supertest": "^7.1.4",
    "typescript": "^5.2.2",
    "vite": "^4.1.4",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.19"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
