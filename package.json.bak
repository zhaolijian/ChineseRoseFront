{
  "name": "chinese-rose-uniapp",
  "version": "1.0.0",
  "description": "阅记 - 纸质书笔记整理助手（uni-app多端版）",
  "main": "main.js",
  "keywords": [
    "uniapp",
    "vue3",
    "typescript",
    "miniprogram",
    "h5",
    "app",
    "reading",
    "notes",
    "ocr",
    "mindmap"
  ],
  "author": "Chinese Rose Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/chinese-rose-uniapp.git"
  },
  "scripts": {
    "dev:mp-weixin": "uni build --platform mp-weixin --mode development --watch",
    "dev:h5": "uni build --platform h5 --mode development --watch",
    "dev:app": "uni build --platform app --mode development --watch",
    "build:mp-weixin": "uni build --platform mp-weixin --mode production",
    "build:h5": "uni build --platform h5 --mode production",
    "build:app": "uni build --platform app --mode production",
    "lint": "eslint . --ext .js,.ts,.vue --fix",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@dcloudio/uni-app": "^3.0.0-4020420231218001",
    "@dcloudio/uni-components": "^3.0.0-4020420231218001",
    "@dcloudio/uni-mp-vue": "^3.0.0-4020420231218001",
    "pinia": "^2.1.7",
    "uview-plus": "^3.2.17",
    "vue": "3.4.21"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.8",
    "@dcloudio/uni-automator": "^3.0.0-4020420231218001",
    "@dcloudio/uni-cli-shared": "^3.0.0-4020420231218001",
    "@dcloudio/uni-stacktracey": "^3.0.0-4020420231218001",
    "@dcloudio/vite-plugin-uni": "^3.0.0-4070620250821001",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "sass": "^1.69.5",
    "sharp": "^0.34.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
